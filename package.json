{
  "name": "h5-designer-server",
  "version": "1.0.0",
  "description": "h5设计平台的服务器",
  "private": true,
  "egg": {
    "typescript": true,
    "declarations": true
  },
  "scripts": {
    "start": "egg-scripts start --port=7010 --daemon --title=egg-server-h5-designer-server",
    "stop": "egg-scripts stop --title=egg-server-h5-designer-server",
    "dev": "egg-bin dev --port=7010",
    "debug": "egg-bin debug",
    "test-local": "egg-bin test",
    "test": "npm run lint -- --fix && npm run test-local",
    "cov": "egg-bin cov",
    "tsc": "ets && tsc -p tsconfig.json",
    "ci": "npm run lint && npm run cov && npm run tsc",
    "autod": "autod",
    "lint": "eslint --fix app/** --ext .ts",
    "clean": "ets clean"
  },
  "dependencies": {
    "dayjs": "^1.10.6",
    "egg": "^2.6.1",
    "egg-blueprint": "^1.2.1",
    "egg-cors": "^2.2.3",
    "egg-jwt": "^3.1.7",
    "egg-scripts": "^2.6.0",
    "egg-sequelize": "^6.0.0",
    "mysql2": "^2.3.0",
    "nodemailer": "^6.6.3"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.40",
    "@types/node": "^7.0.12",
    "@types/supertest": "^2.0.0",
    "autod": "^3.0.1",
    "autod-egg": "^1.1.0",
    "egg-bin": "^4.11.0",
    "egg-ci": "^1.8.0",
    "egg-mock": "^3.16.0",
    "eslint": "^6.7.2",
    "eslint-config-egg": "^8.0.0",
    "tslib": "^1.9.0",
    "typescript": "^3.0.0"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "ci": {
    "version": "8"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tuchongyang/h5-designer-server"
  },
  "eslintIgnore": [
    "coverage"
  ],
  "author": "tcy",
  "license": "MIT"
}
